package com.example.demo.models;

import java.util.ArrayList;
import java.util.List;

import org.springframework.data.annotation.Id;
import org.springframework.data.mongodb.core.mapping.Document;

@Document(collection = "TrainDetails")
public class details {

	private String trainName;
	@Id
	private int trainNumber;
	List<Integer> trainStopsTimes=new ArrayList<Integer>();
	List<String> trainStopName=new ArrayList<String>();
	public String getTrainName() {
		return trainName;
	}
	public void setTrainName(String trainName) {
		this.trainName = trainName;
	}
	public int getTrainNumber() {
		return trainNumber;
	}
	public void setTrainNumber(int trainNumber) {
		this.trainNumber = trainNumber;
	}
	public List<Integer> getTrainStopsTimes() {
		return trainStopsTimes;
	}
	public void setTrainStopsTimes(List<Integer> trainStopsTimes) {
		this.trainStopsTimes = trainStopsTimes;
	}
	public List<String> getTrainStopName() {
		return trainStopName;
	}
	public void setTrainStopName(List<String> trainStopName) {
		this.trainStopName = trainStopName;
	}
	public details(String trainName, int trainNumber, List<Integer> trainStopsTimes, List<String> trainStopName) {
		super();
		this.trainName = trainName;
		this.trainNumber = trainNumber;
		this.trainStopsTimes = trainStopsTimes;
		this.trainStopName = trainStopName;
	}
	public details() {
		super();
	}
	
	
	
	
}
